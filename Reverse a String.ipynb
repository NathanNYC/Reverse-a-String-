   "source": [
    "x = input(\"Enter a word: \")\n",
    "\n",
    "y=len(x)\n",
    "z=y \n",
    "counter=0\n",
    "reverseList=[]\n",
    "\n",
    "while counter < y: \n",
    "    reverseList.append(x[z-1])\n",
    "    z=z-1\n",
    "    counter+=1\n",
    "    \n",
    "Clean = ''.join([str(elem) for elem in reverseList])\n",
    "\n",
    "print(Clean)\n",
    "\n",
    "\n"
   ]
